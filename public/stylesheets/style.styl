*
  padding:0
  margin:0
body
  width:95%
  margin:15px
  padding:0 2em
  background: #EDE8E2;
  font-size:14px
  font-family:"Microsoft YaHei"
a:link
  text-decoration:none
a:visited
  text-decoration:none
a:hover
  text-decoration:underline
a:active
  text-decoration:underline
p
  line-height:24px
  margin:1em 0
h2
  padding: 10px 15px
header
  padding:.5em 0
  border-bottom:1px solid #cccccc
.welcome
  float:right
  padding-right:32px
  padding-top:20px
table
  border-collapse:collapse
  border:1px solid white
  text-align: center
  align: center
  width:100%
  background-color:white
td
  border-top:1px solid #A3C159
th
  border:1px solid white
  background-color:#A3C159
  color:white
nav
  font-family: Arial
  font-size: 14px
  overflow: hidden
  width: 100%
nav, nav ul
  list-style-type: none
  background: #A3C159
  margin: 0
  padding: 0
nav li
  float: left
nav li a
  display: block
  padding: 10px 15px
  color: #FFF
  text-decoration: none
  border-right: 1px solid #FFF
nav li a:hover
  background: #1BA6B2
nav li ul li
  float: none
nav li ul li a
  border-top: 1px solid #FFF
nav li ul
  display: none
  position: absolute
nav li:hover ul
  display: block
content
  height: auto
  line-height: 1.5em
  padding: 10px
section
  display: block
  clear: both
chart
  float:left
  padding: 15px 15px
.pieChart
  height:650px
  width:600px
.barChart
  height:400px
  width:900px
