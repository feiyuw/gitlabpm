<%- include header %>
<% posts.forEach(function(post, index) { %>
    <p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a>
    <p class="info">
        Author: <a href="/u/<%= post.name %>"><%= post.name %></a> | 
        Created: <%= post.time.minute %>
    </p>
    <p><%- post.post %></p>
<% }) %>
<%- include footer %>
